    <div class= "ml-5 col-4">
    <form class="form-signin mt-5 row">
      <h1 class="h3 mt-5 mb-3 font-weight-normal">Inicia sesión</h1>
      <label for="inputEmail" class="sr-only">Correo electronico</label>
      <input type="email" id="inputEmail" class="form-control" placeholder="Correo electronico" required autofocus>
      <label for="inputPassword" class="sr-only">Contraseña</label>
      <input type="password" id="inputPassword" class="form-control mt-2" placeholder="Contraseña" required>
      <div class="checkbox mb-3 mt-1">
        <label>
          <input type="checkbox" value="remember-me"> Recuerdame
        </label>
      </div>
      <button class="btn btn-lg btn-primary btn-block" type="submit">Iniciar sesión</button>
      <a href="/google" class="btn btn-danger mr-2 mt-2 col-4">Iniciar sesión con Google</a>
      <a href="/facebook" class="btn btn-primary mr-2 mt-2 col-4">Iniciar sesión con Facebook</a>
    </form>
